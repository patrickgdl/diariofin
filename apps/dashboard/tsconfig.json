{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "@diariofin/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "lib": ["ES2015", "DOM"],
    "module": "preserve",
    "moduleResolution": "bundler",
    "target": "ES6",
    "jsx": "react-jsx",
    "noEmit": true
  }
}
