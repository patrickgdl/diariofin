{
  "name": "fluxozen",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "dev": "turbo dev --parallel",
    "start:dashboard": "turbo start --filter=@fluxozen/dashboard",
    "start:website": "turbo start --filter=@fluxozen/website",
    "dev:dashboard": "turbo dev --filter=@fluxozen/dashboard",
    "dev:website": "turbo dev --filter=@fluxozen/website ",
    "build:dashboard": "turbo build --filter=@fluxozen/dashboard",
    "build:website": "turbo build --filter=@fluxozen/website"
  },
  "dependencies": {
    "turbo": "^1.13.2"
  }
}